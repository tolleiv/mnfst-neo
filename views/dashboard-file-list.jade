extends layout

block content

    .row
        .col-md-6
            .container-fluid
                .row
                    .panel.panel-default
                        .panel-heading File score distribution
                        .panel-body
                            #fileScoreHisto
                .row
                    .panel.panel-default
                        .panel-heading File score
                        .panel-body
                            #fileScoreDots
                .row
                    .panel.panel-default
                        .panel-heading Entity counts
                        .panel-body
                            #countsChart

        .col-md-6
            .panel.panel-default
                table.table.table-striped
                    thead
                        tr
                            th Files
                            th Influenced servers
                            th Influence score
                    tbody
                        each file in influencialFiles
                            tr
                                td
                                    a(href='/ui/dashboards/file/' + file.id)= file.name
                                td= file.count
                                td= file.score